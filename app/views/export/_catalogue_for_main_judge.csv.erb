<%= dog.start_number %>;<%= dog.handler.first_name %>;<%= dog.handler.last_name %>;<%= Country.find_country_by_number("0" * (3 - dog.handler.country_id.to_s.length)  + dog.handler.country_id.to_s).name  %>;<%= dog.nickname %>;<%= dog.registered_name %>;<%= dog.owner_first_name %>;<%= dog.owner_last_name %>;<%= dog.category %>;<%= Breed.find(dog.breed_id).name %>;<%= dog.microchip %>;<%= dog.date_of_birth %>
