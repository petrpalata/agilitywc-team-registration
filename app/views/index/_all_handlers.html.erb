<% if not @country_name.empty? %>
<% end %>
<% if @handlers.empty? %>
<% else %>
    <br />
    <table class="handlers">
        <th></th>
        <th><%= t 'teams.form.first_name' %></th>
        <th><%= t 'teams.form.last_name' %></th>
        <th><%= t 'activerecord.models.dog' %></th>
        <th><%= t 'activerecord.attributes.team.category' %></th>
        <th><%= t 'activerecord.attributes.dog.breed_id' %></th>
        <% @handlers.each do |handler| %>
            <% handler.dogs.all.each do |dog| %>
                <tr>
                    <td><img src="http://agility2012.cz/team_registration<%= handler.picture.url(:big_thumb) %>" /></td>
                    <td><%= handler.first_name %></td>
                    <td><%= handler.last_name %></td>
                    <td><%= dog.registered_name %></td>
                    <td><%= dog.category %></td>
                    <td><%= Breed.find(dog.breed_id).name %></td>
                </tr>
            <% end %>
        <% end %>
    </table>
<% end %>
